pipeline {
  agent any 
    stages {
      stage('Hi') {
        steps {
           echo 'Hello Wordl'
        }
      }
      stage('Build') {
        steps {
          echo 'Building'
        }
      }
      stage('Test') {
        steps {
          echo 'Testing'
        }
      }
      stage('Package') {
        steps {
          echo 'Packaging'
        }
      }
      stage('Deploy') {
        steps {
          echo 'Deploying'
        }
      }
    }
  }
